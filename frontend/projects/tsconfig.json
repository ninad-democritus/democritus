{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "..",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "module": "es2022",
    "resolveJsonModule": true,
    "target": "es2022",
    "useDefineForClassFields": false,
    "paths": {
      "shared-ui": [
        "dist/shared-ui"
      ],
      "ui-kit": [
        "dist/ui-kit"
      ]
    },
    "lib": [
      "es2022",
      "dom"
    ]
  }
}


